<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
      <extension point="org.eclipse.ui.commands">
            <category id="org.eclipse.ui.views.midi.commandCategories.Midi" name="MIDI"/>
            <command id="org.eclipse.ui.views.midi.commands.PlayPause" name="Play/Pause" categoryId="org.eclipse.ui.views.midi.commandCategories.Midi">
            	<commandParameter id="org.eclipse.ui.views.midi.commands.PlayPause.view" name="View" optional="false" values="org.eclipse.ui.views.midi.PlayPauseViewParameterValues"/>
